	  <script>
		var ctx = document.getElementById('thecanvas').getContext('2d');
			  if(ctx.ellipse)
			  {
				  ctx.save();
				  ctx.beginPath();
				  ctx.arc(400, 170, 200, 5/6*Math.PI, 1/6*Math.PI, true);
				  ctx.globalAlpha = .63;
				  ctx.fillStyle='#F2E1E7';
				  ctx.fill();
				  ctx.restore();
			  }
		</script>
